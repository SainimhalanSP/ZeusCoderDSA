import java.util.Scanner; 
public class Main { 
    public static void main (String[] args) { 
        Scanner sc= new Scanner(System.in); 
        String a= sc.next();  
        String b= sc.next(); 
        int n= sc.nextInt(); 
        String fullname=a+b; 
        for (int i=0; i<n; i++) { 
            System.out.println(fullname);} 
    } 
} 
