import java.util.Scanner; 
public class Main { 
    public static void main(String[]args) { 
    Scanner sc=new Scanner(System.in); 
    int a= sc.nextInt(); 
  
    if (a==29) {System.out.println("Small");} 
    else if (a==30) {System.out.println("Medium");} 
    else if (a==38) {System.out.println("Large");} 
    else if (a==42) {System.out.println("Xlarge");} 
    else {System.out.println("Invalid output");} 
    } 
    
}
